.ta_btn_primary {
    display: block;
    width: 10em;
    height: 2.4em;
    background-color: #cc0000;
    color: #fff;
    -webkit-border-radius: 1.2em;
    -moz-border-radius: 1.2em;
    border-radius: 1.2em;
}
.ta_cancel{
    display: block;
    width: 10em;
    height: 2.4em;
    background-color: #a0a0a0;
    color: #fff;
    -webkit-border-radius: 1.2em;
    -moz-border-radius: 1.2em;
    border-radius: 1.2em;
}
