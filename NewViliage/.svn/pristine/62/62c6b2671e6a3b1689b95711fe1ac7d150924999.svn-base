#div1{
	z-index:102;
	position:fixed;
	top:150px;
	left:50%;
	width:80%;
	margin-left:-42%;
	box-shadow: 0 0 10px #999;
	background-color:#fff;	
	padding:10px 10px 10px 10px;
	display: none;
}
#div1 form{
	text-align: center;
}
#div1 textarea{
	width: 100%;
	height: 50px;
}
#div1 button{
	width: 100px;
	height: 30px;
	font-size: 14px;
	font-weight: bold;
	margin: 0 auto;
}
.variety_list_sub_div{
	display:inline-block;
	padding:5px 10px 5px 10px;
	margin:0px 10px 10px 0px;
	background-color:#f5f5f5;
	text-wrap:none;
}


#div2{
	border-radius: 6px;
    margin: 10px 10px 0px 10px;
    padding: 10px 10px 10px 10px;
    background-color: #00AAEE;
    outline: none;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
    text-align: center;
    font: normal 2em "微软雅黑";
    letter-spacing: 5px;
    color: #ffffff
}
