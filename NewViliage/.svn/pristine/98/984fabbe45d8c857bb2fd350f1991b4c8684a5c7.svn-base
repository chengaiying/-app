package com.moa.viliage.model;

import java.util.List;

public class TableResourceModel {
	// 目录id
	private String tableResourceId;
	// 目录关联code
	private String tableResourceCode;
	// 目录名称
	private String tableResourceName;
	// 父级目录名称
	private String tableResourceParentName;
	// 目录级别
	private String type;

	public String getTableResourceCode() {
		return tableResourceCode;
	}

	public void setTableResourceCode(String tableResourceCode) {
		this.tableResourceCode = tableResourceCode;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	private List<TableResourceModel> children;

	public String getTableResourceId() {
		return tableResourceId;
	}

	public void setTableResourceId(String tableResourceId) {
		this.tableResourceId = tableResourceId;
	}

	public String getTableResourceName() {
		return tableResourceName;
	}

	public void setTableResourceName(String tableResourceName) {
		this.tableResourceName = tableResourceName;
	}

	public String getTableResourceParentName() {
		return tableResourceParentName;
	}

	public void setTableResourceParentName(String tableResourceParentName) {
		this.tableResourceParentName = tableResourceParentName;
	}

	public List<TableResourceModel> getChildren() {
		return children;
	}

	public void setChildren(List<TableResourceModel> children) {
		this.children = children;
	}
}
