		function fixedInputBlur() {
			var beforeIpt = null;
			$("input").on("focus", function(e) {
				beforeIpt = this;
			});
			$(document).on("tap", function(e) {
				if(e.target.nodeName.toLowerCase() != "input") {
					if(beforeIpt) {
						beforeIpt.blur();
						beforeIpt = null;
					}
					// a标签也能获取焦点，但是用代码模拟点击事件却不能触发键盘隐藏
				}
			});
		}
