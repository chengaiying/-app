										if(value.dataType == 6){
											val = value.value
											var a = val.substring(val.indexOf("[") + 1, val.indexOf("]"))
											var b = a.split(",");
											for(var i = 0; i <= b.length - 1; i++) {
												str = b[i].replace("\"", "").replace("\"", "");
												$("#check_" + n + " input[value='" + str + "']").attr("checked", true);
											}
										};
									});
								};
						});
					}
				}
			});
