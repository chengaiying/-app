@charset "utf-8";
/* CSS Document */

.sell_group{
	width:100%;
	background-color:#fff;
	border-top:solid 1px #e5e5e5;
	margin-top:10px;
}

.sell_type_name{
	font: normal 1.6em "微软雅黑";
	padding:0px 10px 0px 10px;
	margin:10px 10px 0px 10px;
	color:#ff8108;
	border-left:solid 3px #ff8108;
}


.sell_table{
	width:100%;
	background-color:#fff;
	margin-bottom:10px;
	border-bottom:solid 1px #e5e5e5;
}

.sell_img{
	
}

.sell_img img{
	
}

.sell_title{
	font: normal 1.6em "微软雅黑";
	color:#000;
	padding:10px 10px 0px 10px;
}

.sell_title img{
	height:18px;
	padding-right:2px;
	vertical-align:top;
}

.sell_introduce{
	font: normal 1.4em "微软雅黑";
	color:#a5a5a5;
	padding:8px 10px 10px 10px;
}

.sell_goto_table{
	background-color:#FFF;
	padding:10px 0 10px 10px;
	border-bottom:solid 1px #e5e5e5;
}

.sell_goto_content img{
	width:50px;
	padding-right:10px;
	margin-right:16px;
	border-right:solid 1px #f0f0f0;
}

.sell_goto_header{
	display:inline;
	font: normal 1.4em "微软雅黑";
	color:#fff;
	border-radius:6px;
	background-color:#ff8108;
	padding:4px 6px 4px 6px;
}

.sell_goto_intro{
	font: normal 1.4em "微软雅黑";
	color:#a5a5a5;
	margin-top:8px;
}

.sell_table2{
	width:100%;
	border-bottom:dashed 1px #e5e5e5;
}

.sell_img2{
	padding:10px 10px 10px 10px;
	width:1%;
}

.sell_img2 img{
	width:80px;
}

.sell_title2{
	font: normal 1.6em "微软雅黑";
	color:#000;
	padding:10px 10px 0px 0px;
}

.sell_title2 img{
	height:18px;
	padding-right:2px;
	vertical-align:top;
}

.sell_introduce2{
	font: normal 1.4em "微软雅黑";
	color:#a5a5a5;
	padding:8px 10px 10px 0px;
}

.sell_content_group{
	background-color:#fff;
	border-top:solid 1px #e5e5e5;
	border-bottom:solid 1px #e5e5e5;
	padding:0px 10px 10px 10px;
}

.sell_content_group h1{
	font: normal 1.6em "微软雅黑";
	color:#333;
	line-height:1.6em;
	padding:0;
	margin:10px 0px 4px 0px;
}

.sell_content_group p{
	font: normal 1.4em "微软雅黑";
	color:#666;
	line-height:1.4em;
	padding:0px 0px 10px 0px;
	margin:0px 0px 0px 0px;
	border-bottom: dashed 1px #e5e5e5;
}

.sell_content_group a,.sell_content_group a:link,.sell_content_group a:visited,.sell_content_group a:hover{
	color:#ff8108;
}


.policy_into_line{
	font: normal 1.8em "微软雅黑";
	width:100%;
	background-color:#fff;
	border-top:solid 1px #e5e5e5;
	border-bottom:solid 1px #e5e5e5;
}

.policy_into_line img{
	height:36px;
	padding:0px 10px 0px 10px;
	margin:8px 0px 8px 0px;
	margin-right:10px;
	border-right:dashed 1px #ff8108;
}

.policy_into_title{
	color:#ff8108;
	padding-top:6px;
}

.policy_into_tips{
	color:#a0a0a0;
	font-size:0.8em;
}

.policy_group{
	width:100%;
	background-color:#fff;
	border-top:solid 1px #e5e5e5;
	margin-top:10px;
}

.policy_type_name{
	font-size: 2em;
	padding: 0px 10px 0px 0px;
	margin: 10px 10px 0px 10px;
	color: #00aaee;
	letter-spacing: 2px;
	display:inline;
	float:left;
}

.policy_more{
	font:normal 1em "微软雅黑";
	display:inline;
	float:right;
	color:#a0a0a0;
	padding:2px 2px 1px 2px;
	margin:12px 10px 0 0;	
	
	border-radius:4px;
	border:solid 1px #e5e5e5;
}

.policy_table{
	width:100%;
	border-bottom:dashed 1px #e5e5e5;
}

.policy_img{
	padding:10px 10px 10px 10px;
	width:1%;
}

.policy_img img{
	width:80px;
	height:60px;
}

.policy_title{
	font: normal 1em "微软雅黑";
	color:#000;
	padding:10px 10px 0px 0px;
}

.policy_comment{
	color:#a0a0a0;
	padding:0px 10px 10px 0px;
	text-align:right;
}

.policy_comment img{
	height:14px;
	margin-left:6px;
}




.my_score_table{
	background-color:#fff;
}

.my_score_avatar{
	padding-top:50px;
	padding-bottom:50px;
	
}

.my_score_avatar img{
	width:100px;
	height:100px;
	border-radius:100px;
	border:solid 2px #c3e1fb;
}